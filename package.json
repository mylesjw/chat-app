{
  "name": "mern-chat-app",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "server": "nodemon --exec babel-node server.js",
    "server:build": "tsc",
    "server:build:dev": "tsc -w",
    "server:start": "node ./dist/server.js",
    "server:start:dev": "nodemon ./dist/server.js",
    "server:dev": "concurrently \"npm run server:build:dev\" \"npm run server:start:dev\"",
    "client:dev": "npm run dev --prefix client",
    "client:build": "npm run build --prefix client",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\""
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.1",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "@types/express": "^4.17.6",
    "@types/mongoose": "^5.7.27",
    "@types/socket.io": "^2.1.8",
    "concurrently": "^5.2.0",
    "css-loader": "^3.5.3",
    "nodemon": "^2.0.4",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.5",
    "tslint": "^6.1.2",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.19",
    "socket.io": "^2.5.0"
  }
}
